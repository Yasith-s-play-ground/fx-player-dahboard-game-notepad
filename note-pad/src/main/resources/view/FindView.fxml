<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.RadioButton?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.ToggleGroup?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="262.0" prefWidth="583.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="lk.ijse.dep12.fx.note_pad.controller.FindViewController">
   <children>
      <Label layoutX="63.0" layoutY="38.0" prefHeight="24.0" prefWidth="81.0" text="Find What :" />
      <TextField fx:id="txtFind" layoutX="173.0" layoutY="38.0" prefHeight="24.0" prefWidth="257.0" />
      <Button fx:id="btnFindNext" layoutX="449.0" layoutY="38.0" mnemonicParsing="false" onAction="#btnFindNextOnAction" text="Find Next" />
      <Button fx:id="btnCancel" layoutX="449.0" layoutY="79.0" mnemonicParsing="false" onAction="#btnCancelOnAction" prefHeight="24.0" prefWidth="81.0" text="Cancel" />
      <Label layoutX="324.0" layoutY="103.0" prefHeight="24.0" prefWidth="81.0" text="Direction" />
      <RadioButton fx:id="rdBtnUp" layoutX="324.0" layoutY="137.0" mnemonicParsing="false" onAction="#rdBtnUpOnAction" prefHeight="18.0" prefWidth="61.0" text="Up">
         <toggleGroup>
            <ToggleGroup fx:id="rdBtnGroup" />
         </toggleGroup>
      </RadioButton>
      <RadioButton fx:id="rdBtnDown" layoutX="405.0" layoutY="137.0" mnemonicParsing="false" prefHeight="18.0" prefWidth="61.0" selected="true" text="Down" toggleGroup="$rdBtnGroup" />
      <CheckBox fx:id="chkBoxMatchCase" layoutX="63.0" layoutY="168.0" mnemonicParsing="false" onAction="#chkBoxMatchCaseOnAction" text="Match Case" />
      <CheckBox fx:id="chkBoxWrapAround" layoutX="63.0" layoutY="204.0" mnemonicParsing="false" text="Wrap Around" />
   </children>
</AnchorPane>
